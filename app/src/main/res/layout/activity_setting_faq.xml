<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_faq"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.hyochan.dontech.activities.SettingFAQActivity">
    <include
        android:id="@+id/rel_title"
        layout="@layout/title_back"/>
    <FrameLayout
        android:layout_below="@+id/rel_title"
        android:background="@color/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!-- cacheColorHint : 스크롤시 검은색 배경을 없애는 기능 -->
        <ExpandableListView
            android:id="@+id/exp_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="#b5b5b5"
            android:dividerHeight="0.5dp"
            android:cacheColorHint="#00000000"
            android:listSelector="@drawable/sel_btn"
            android:groupIndicator="@null"
            />
    </FrameLayout>
</RelativeLayout>
